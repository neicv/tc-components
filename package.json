{
  "name": "tc-components",
  "version": "0.22.3",
  "description": "test Mithril Component for TC",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neicv/tc-components.git"
  },
  "scripts": {
    "test": "npm run editorconfig && mocha --recursive test/unit/",
    "editorconfig": "editorconfig-checker",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "makefont": "rimraf ./dist && webpack --mode production --config ./webpack.font.config.js && npm run generate && npm run copyfont && npm run url",
    "generate": "node ./font_generator/src/build.js",
    "copyfont": "node ./font_generator/src/copy.js",
    "url": "node ./font_generator/src/cssCorrection.js"
  },
  "keywords": [
    "js",
    "html",
    "mithril"
  ],
  "author": "neicv",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.15.3",
    "@webpack-cli/serve": "^1.6.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsx-control-statements": "^4.1.2",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.5",
    "editorconfig-checker": "^4.0.2",
    "file-loader": "^6.0.0",
    "fs-extra": "^10.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mithril": "^2.0.0",
    "mocha": "^9.2.2",
    "mocha-list-tests": "^1.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "sass-loader": "^10.1.1",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "webfonts-loader": "^7.4.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev": "^1.1.1",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "commonjs-variables-for-esmodules": "^1.1.0",
    "highlight.js": "^11.2.0",
    "immutability-helper": "^3.1.1",
    "midux": "^2.2.0",
    "mithril": "^2.0.0",
    "normalize.css": "^8.0.1",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "util": "^0.12.4"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
